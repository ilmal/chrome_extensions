{
    "manifest_version": 3,
    "name": "Inet Percent Viewer",
    "description": "This is a tool that shows the percentage of a discount",
    "version": "1.0",
    "content_scripts": [
        {
            "js": [
                "scripts/main.js"
            ],
            "matches": [
                "https://www.inet.se/*"
            ]
        }
    ],
    "action": {
        "default_icon": "i_icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://www.inet.se/*"
    ]
}